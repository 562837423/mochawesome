{
  "name": "mochawesome",
  "version": "2.0.0",
  "description": "A Gorgeous HTML/CSS Reporter for Mocha.js",
  "scripts": {
    "lint": "eslint lib test",
    "test": "npm run lint && cross-env NODE_ENV=test nyc mocha",
    "test:fn": "mocha test-functional/test.js --opts test-functional/mocha.opts",
    "build": "babel lib --out-dir dist"
  },
  "author": "Adam Gruber",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adamgruber/mochawesome"
  },
  "keywords": [
    "mocha",
    "reporter",
    "json",
    "html"
  ],
  "engine": "node >= 0.10.0",
  "main": "dist/mochawesome.js",
  "nyc": {
    "include": [
      "lib/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "lcov",
      "html",
      "text-summary"
    ],
    "cache": true,
    "check-coverage": true,
    "lines": 99,
    "statements": 99,
    "functions": 100,
    "branches": 90
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "chalk": "^1.1.3",
    "diff": "^3.0.0",
    "highlight.js": "^9.6.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.15.0",
    "mkdirp": "^0.5.1",
    "node-uuid": "^1.4.7"
  },
  "peerDependencies": {
    "mocha": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-3": "^6.11.0",
    "babel-register": "^6.14.0",
    "cross-env": "^2.0.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^1.14.0",
    "mocha": "*",
    "nyc": "^8.1.0",
    "proxyquire": "^1.7.10",
    "should": "^3.3.2",
    "sinon": "^1.17.5"
  }
}
