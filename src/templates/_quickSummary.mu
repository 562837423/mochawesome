<ul class="list-unstyled quick-summary">
  <li class="qs-item summary-suites">{{suites}}</li>
  <li class="qs-item summary-tests">{{testsRegistered}}</li>
  <li class="qs-item summary-passes" data-filter="passed">{{passes}}</li>
  <li class="qs-item summary-failures" data-filter="failed">{{failures}}</li>
  <li class="qs-item summary-pending" data-filter="pending">{{pending}}</li>
  <li class="qs-item summary-duration">{{formatSummaryDuration duration}}</li>
</ul>